{
  "gateway": {
    "port": 2998,
    "enabled": true
  },
  "providers": {
    "huggingface": {
      "baseUrl": "https://api-inference.huggingface.co/models",
      "apiKey": "$HF_TOKEN",
      "models": {
        "huggingface/meta-llama/Meta-Llama-3.1-8B-Instruct": {
          "enabled": true,
          "pricing": {
            "prompt": 0,
            "completion": 0
          },
          "options": {
            "max_tokens": 512,
            "temperature": 0.7
          }
        }
      }
    }
  },
  "agents": {
    "defaults": {
      "model": "huggingface/meta-llama/Meta-Llama-3.1-8B-Instruct",
      "models": {
        "cloud": "huggingface/meta-llama/Meta-Llama-3.1-8B-Instruct"
      }
    }
  },
  "platforms": {
    "telegram": {
      "enabled": true,
      "bots": [
        {
          "token": "$TELEGRAM_BOT_TOKEN",
          "username": "chunmunn_bot"
        }
      ]
    }
  }
}
